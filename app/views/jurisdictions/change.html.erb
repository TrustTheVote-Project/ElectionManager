<div class="block">
    <div class="content">
        <h2 class="title">Choose Your Jurisdiction:</h2>
        <div class="inner">
            <p class="first">
                Welcome to the TrustTheVote Election Manager! <% if can? :register, User %>
                You may access public information here without logging in. 
                If you would likea free account, click 
                <%= link_to "here", register_user_path %>to register.<% end %>
                Thank you for your interest!
            </p>
            <ul class="list">
                <% @district_sets.each do |district_set| %>
                <li>
                    <%= render :partial => 'jurisdiction_row', :locals => {:district_set => district_set }  %>
                </li>
                <% end %>
            </ul>
            <br/>
        </div>
    </div>
    <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        <%= will_paginate @district_sets %>
    </div>
</div>
<% content_for :sidebar, render(:partial => 'sidebar') -%>
