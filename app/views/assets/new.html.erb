<div class="block">
  <div class="content">            
    <h2 class="title"><%= t("ttv.new", :default => "New")%>  Asset</h2>
    <div class="inner">
      <% form_for :asset, @asset, :url => assets_path, :html => { :class => :form, :multipart => true } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
