<h1>New election</h1>

<% form_for(@election) do |f| %>
  <%= render_error_messages(@election) %>

  <p>
    <%= f.label :display_name %><br />
    <%= f.text_field :display_name %>
  </p>
	<p>
	  <%= f.label :start_date %><br />
		<%= datetime_select :election, :start_date, { :start_year => 2009 }%>
	</p>
  <p>
    <%= f.label :district_set_id %><br />
		<%= select("election", "district_set_id", DistrictSet.all.collect {|p| [ p.display_name, p.id ] }) %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
