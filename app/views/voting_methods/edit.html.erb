<h1>Editing voting_method</h1>

<% form_for(@voting_method) do |f| %>
  <%= render_error_messages(@voting_method) %>

  <p>
    <%= f.label :display_name %><br />
    <%= f.text_field :display_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @voting_method %> |
<%= link_to 'Back', voting_methods_path %>