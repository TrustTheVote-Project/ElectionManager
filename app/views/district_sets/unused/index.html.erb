<h1>Listing district_sets</h1>

<table>
  <tr>
    <th>Display name</th>
  </tr>

<% @district_sets.each do |district_set| %>
  <tr>
    <td><%=h district_set.display_name %></td>
    <td><%= link_to 'Show', district_set %></td>
    <td><%= link_to 'Edit', edit_district_set_path(district_set) %></td>
    <td><%= link_to 'Destroy', district_set, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New district_set', new_district_set_path %>