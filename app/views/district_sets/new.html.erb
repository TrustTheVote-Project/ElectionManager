<h1>New district_set</h1>

<% form_for(@district_set) do |f| %>
  <%= render_error_messages(@district_set) %>

  <p>
    <%= f.label :display_name %><br />
    <%= f.text_field :display_name %>
  </p>
  <p>
    <%= f.label :secondary_name %><br />
    <%= f.text_field :secondary_name %>
  </p>  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', district_sets_path %>