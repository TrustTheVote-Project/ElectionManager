    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
										<th>
					<%= t("activerecord.attributes.precinct.precict", :default => t("activerecord.labels.precinct", :default => "Precinct")) %>
</th>
					          <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @precincts.each do |precinct| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= precinct.id %>
          </td>
										<td>
						<%= link_to precinct.display_name, precinct_path(precinct) %>
					</td>
										<td>
            <%= precinct.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.ballot", :default => "Ballot")}", precinct_path(precinct) %> |
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", precinct_path(precinct) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_precinct_path(precinct) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", precinct_path(precinct), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
        <%= will_paginate @precincts  %>
        
      </div>
    </div>
